{
    // Use IntelliSense para saber los atributos posibles.
    // Mantenga el puntero para ver las descripciones de los existentes atributos.
    // Para más información, visite: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Stalker (Debug w/ Google Chrome)",
            "type": "stalker",
            "request": "launch",
            "vars": {
                "framework": "net9.0",
                "projectDir": "${workspaceFolder}",
                "projectName": "prueba"
            },
            "project": "${var:projectDir}/prueba/${var:projectName}.csproj",
            "process": "${var:projectDir}/prueba/bin/Debug/${var:framework}/${var:projectName}",
            "cwd": "${var:projectDir}",
            "url": "https://localhost:7296",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "webRoot": "${var:projectDir}/prueba/wwwroot",
            "attachOptions": {
                "action": "debugWithChrome",
                "urlPath": "https://localhost:7296",
                "interval": 500
            }
        }
    ]
}